+PyObject* appGetLocalTime(PyObject* poSelf, PyObject* poArgs)
+{
+	DWORD dwTime;
+	if (!PyTuple_GetUnsignedLong(poArgs, 0, &dwTime))
+		return Py_BuildException();
+
+	const time_t time = static_cast<time_t>(dwTime);
+	struct tm tm_buf;
+	localtime_s(&tm_buf, &time);
+
+	PyObject* dict = PyTuple_New(6);
+	PyTuple_SetItem(dict, 0, PyInt_FromLong(tm_buf.tm_year + 1900));
+	PyTuple_SetItem(dict, 1, PyInt_FromLong(tm_buf.tm_mon + 1));
+	PyTuple_SetItem(dict, 2, PyInt_FromLong(tm_buf.tm_mday));
+	PyTuple_SetItem(dict, 3, PyInt_FromLong(tm_buf.tm_hour));
+	PyTuple_SetItem(dict, 4, PyInt_FromLong(tm_buf.tm_min));
+	PyTuple_SetItem(dict, 5, PyInt_FromLong(tm_buf.tm_sec));
+	return dict;
+}
+
+PyObject* appGetTimeString(PyObject* poSelf, PyObject* poArgs)
+{
+	DWORD dwTime;
+	if (!PyTuple_GetUnsignedLong(poArgs, 0, &dwTime))
+		return Py_BuildException();
+
+	const time_t time = static_cast<time_t>(dwTime);
+	struct tm tm_buf;
+	localtime_s(&tm_buf, &time);
+
+	char buf[256];
+	snprintf(buf, sizeof(buf), "%04d/%02d/%02d %02d:%02d:%02d", tm_buf.tm_year + 1900, tm_buf.tm_mon + 1, tm_buf.tm_mday, tm_buf.tm_hour, tm_buf.tm_min, tm_buf.tm_sec);
+	return Py_BuildValue("s", buf);
+}
